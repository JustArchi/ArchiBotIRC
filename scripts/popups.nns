  ...$submenu($mask.sub($1,ignore,$ial(%query.popnick)))
  .$iif(!$ial($1) || $address($1,%ban.mask) iswm $address($me,5),$style(2),$iif($hfind(black,$address($1,5),0,W),$style(1))) &Blacklist
