alias pclones {
  var %a = 1,%i = $ialchan($1,$chan,0),%r
  while (%a <= %i) {
    var %n = $ialchan($1,$chan,%a).nick
    if (%n != $me) {
      %r = %r %n
      if ($len(%r) > $iif($prop == short,40,500)) {
        %r = %r ...
        break
      }
    }
    inc %a
  }
  return $replace(%r,$chr(32),$chr(44) $+ $chr(32))
}
